{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ports": {
    "http": 7562,
    "https": 7563
  },
  "certificate": {
    "path": "NA",
    "password": ""
  },
  "UseAd": false,
  "ShortCacheTime": 60,
  "StaticPaths": [ "assets", "logs" ],
  "Storage": {
    "MyDb": {
      "Name": "db1",
      "UseInMemory": false,
      "host": "localhost",
      "port": 5432,
      "username": "dbuser",
      "password": "password",
      "ConnectionStringTemplate": "Host={{host}};Port={{port}};Database={{database}};Username={{username}};Password={{password}};Ssl Mode=Prefer;Trust Server Certificate=true",
      "MigrationsAssemblyName": "Telstra.Core.Data",
      "DatabaseName": "mydb",
      "Schema": "public"
    }
  },
  "AzureAd": {
    "ClientId": "",
    "Instance": "",
    "TenantId": "",
    "Policy": "",
    "KeyInApplicationSettings": ""
  },
  "AzureMapsAuthCredentials": {
    "MsClientId": "{{msClientId}}",
    "AuthUri": "https://login.microsoftonline.com/{{tenantId}}/oauth2/token",
    "ClientId": "{{clientId}}",
    "ClientSecret": "{{clientSecret}}",
    "GrantType": "{{grantType}}",
    "Resource": "{{resource}}"
  },
  "StorageAppGrpc": {
    "BaseUri": "http://localhost:7565"
  },
  "StorageAppHttp": {
    "BaseUri": "http://localhost:7564/api"
  },
  "DeviceManagementCredentials": {
    "BaseUri": "",
    "AccessTokenUri": "",
